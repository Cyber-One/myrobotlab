{
  "name": "myrobotlab-webgui",
  "private": true,
  "version": "0.0.0",
  "description": "The official WebGui for MyRobotLab",
  "repository": "https://github.com/MyRobotLab/myrobotlab",
  "license": "Apache-2.0",
  "dependencies": {
    "bower": "^1.7.9",
    "http-server": "^0.9.0",
    "jasmine-core": "^2.4.1",
    "karma": "^0.13.22",
    "karma-chrome-launcher": "^1.0.1",
    "karma-coverage": "^1.0.0",
    "karma-firefox-launcher": "^1.0.0",
    "karma-jasmine": "^1.0.2",
    "karma-junit-reporter": "^1.0.0",
    "protractor": "^3.3.0",
    "shelljs": "^0.7.0",
	"mrl-bower-install-plus": "file:./mrl-bower-install-plus"
  },
  "scripts": {
    "postinstall": "bower install && \"node_modules/.bin/mrl-bower-install-plus\"",
    "prestart": "npm install",
    "start": "http-server -a localhost -p 8000 -c-1 ./app",
    "pretest": "npm install",
    "test": "karma start karma.conf.js",
    "test-single-run": "karma start karma.conf.js --single-run",
    "preupdate-webdriver": "npm install",
    "update-webdriver": "webdriver-manager update",
    "preprotractor": "npm run update-webdriver",
    "protractor": "protractor e2e-tests/protractor.conf.js"
  }
}
